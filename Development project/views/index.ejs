}

.categories .c1 {
    height: 130px;
    width: 130px;
    border-radius: 50%;
    margin-bottom: 10px;
    overflow: hidden;  /* Ensures anything outside the circle is hidden */
    display: flex;
    background: linear-gradient(180deg, rgba(47, 18, 18, 0) 39.73%, rgba(47, 18, 18, 0.3) 87.78%), #CB9677;
    box-shadow: 0 5px 8px rgba(6, 6, 6, 0.3);

}


.categories img{
    height: 130px;
    width: 130px;
    object-fit:cover;
}

.categories .circle-layer {
    /* background-color: whitesmoke; */
  width: 140px;  /* Set the width of the circle */
  height: 140px; /* Set the height of the circle */ /* Makes it a circle */
  overflow: hidden;  /* Ensures anything outside the circle is hidden */
  display: flex;
  align-items: center;
  justify-content: center;
}

.category-list {
    display: flex;
    overflow-x: auto;
    gap: 40px;
    scroll-behavior: smooth;
    padding: 5px 0;
}

/* Hide scrollbar */
.category-list::-webkit-scrollbar {
    display: none;
}

.category-list {
    -ms-overflow-style: none;
    scrollbar-width: none;
}

.category {
    color: #000;
    padding: 15px;
    border-radius: 5px;
    text-align: center;
    width: 150px;
    font-size: 15px;
    font-weight: bold;
    display: flex;            /* Use flexbox to center content */
    flex-direction: column;
    align-items: center;
}
.mr-2{
